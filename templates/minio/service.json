{
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
        "namespace": "$K8NAMESPACE",
        "name": "minio",
        "labels": {
            "app": "minio"
        }
    },
    "spec": {
        "type": "ClusterIP",
        "ports": [
            {
                "port": 9000,
                "targetPort": 9000
            }
        ],
        "selector": {
            "app": "minio"
        }
    }
}